    "@nestjs/platform-express": "10.2.10",
    "@nestjs/platform-socket.io": "10.2.10",
    "@nestjs/websockets": "10.2.10",
    "@socket.io/redis-adapter": "8.2.1",
    "hbs": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io": "4.7.2",
    "whatsapp-web.js": "^1.23.0"
  },
  "devDependencies": {
    "@dp/config": "workspace:*",
    "@nestjs/cli": "^9.0.0",
